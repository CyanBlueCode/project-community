{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": ["error", { "allow": ["warn", "error", "info"] }],
    "func-names": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "react/jsx-filename-extension": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
